{
    "name": "endeavors/maxmd-proofing",
    "description": "Provide an easy api to integrate with maxmd",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Rodriguez",
            "email": "adam_rodriguez@bitbucket.org"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bpedroza/maxmd-http.git"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bpedroza/maxmd-support.git"
        }
    ],
    "require": {
        "endeavors/maxmd-http": "psr7-dev",
        "endeavors/maxmd-apiauth": "*",
        "endeavors/maxmd-support": "developer-options-dev",
        "endeavors/support-vo": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Endeavors\\MaxMD\\Proofing\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Endeavors\\MaxMD\\Proofing\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.3.x-dev"
        }
    }
}
